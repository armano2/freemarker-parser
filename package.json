{
  "name": "freemarker-parser",
  "version": "1.1.6",
  "description": "Freemarker Parser is a javascript implementation of the Freemarker",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\"",
    "format-check": "prettier --list-different \"./**/*.{ts,js,json,md}\"",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "eslint . --ext .js,.ts --fix",
    "watch": "tsc --watch"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/armano2/freemarker-parser.git"
  },
  "keywords": [
    "freemarker",
    "parser",
    "js",
    "ast",
    "ftl"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Armano (https://github.com/armano2)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/armano2/freemarker-parser/issues"
  },
  "homepage": "https://github.com/armano2/freemarker-parser#readme",
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.24",
    "@types/node": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {}
}
