{
  "name": "freemarker-parser",
  "version": "1.0.1",
  "description": "Freemarker Parser is a javascript implementation of the Freemarker",
  "scripts": {
    "build": "npm run lint && tsc && rollup -c && dts-bundle --name freemarker-parser --main .temp/index.d.ts --out ../index.d.ts",
    "mocha": "mocha \"test/*.js\" --reporter=dot --timeout 5000",
    "lcov": "nyc report --reporter=lcovonly",
    "lint": "eslint ./test && tslint './src/**/*.ts'",
    "pretest": "npm run build",
    "test": "nyc npm run mocha",
    "posttest": "npm run lcov && codecov -f coverage/*.info",
    "update-ast": "node ./scripts/update-ast.js",
    "watch": "run-p watch:*",
    "watch:tsc": "tsc --watch",
    "watch:rollup": "wait-on .temp/index.js && rollup -c --watch",
    "watch-test": "wait-on index.js && warun index.js \"test/*.js\" \"test/**/*.ftl\" -- npm run mocha",
    "coverage": "nyc npm run mocha && nyc report --reporter lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/armano2/freemarker-parser.git"
  },
  "keywords": [
    "freemarker",
    "parser",
    "js",
    "ast",
    "ftl"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "Armano (https://github.com/armano2)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/armano2/freemarker-parser/issues"
  },
  "main": "index.js",
  "files": [
    "index.d.ts",
    "index.js",
    "index.js.map"
  ],
  "homepage": "https://github.com/armano2/freemarker-parser#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "assert": "^1.4.1",
    "chalk": "^2.1.0",
    "codecov": "^3.1.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.1",
    "nyc": "^13.1.0",
    "rollup": "^0.66.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-watch": "^4.3.1",
    "tslint": "^5.7.0",
    "typescript": "^3.1.3",
    "wait-on": "^3.1.0",
    "warun": "^1.0.0"
  },
  "dependencies": {
    "debug": "^4.1.0",
    "line-column": "^1.0.2"
  }
}
