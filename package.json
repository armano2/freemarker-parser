{
  "name": "freemarker",
  "version": "1.0.0",
  "description": "Freemarker Parser is a javascript implementation of the Freemarker",
  "scripts": {
    "_mocha": "mocha \"test/*.js\" --reporter dot --timeout 5000",
    "build": "tsc && rollup -c && dts-bundle --name freemarker-parser --main .temp/index.d.ts --out ../index.d.ts",
    "test": "nyc npm run _mocha",
    "lint": "eslint ./test && tslint './src/**/*.ts'",
    "pretest": "npm run build",
    "posttest": "nyc report --reporter=lcovonly && codecov -f coverage/*.info",
    "update-ast": "node ./scripts/update-ast.js",
    "watch-ast": "wait-on index.js && warun index.js \"test/data/*/*.ftl\" -- ",
    "watch": "run-p watch:*",
    "watch:tsc": "tsc --watch",
    "watch:rollup": "wait-on .temp/index.js && rollup -c --watch",
    "watch2:ast": "npm run watch-ast -- npm run update-ast",
    "watch2:test": "npm run watch-ast -- npm run _mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/armano2/freemarker-parser.git"
  },
  "keywords": [
    "freemarker",
    "js",
    "ast",
    "ftl"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "Armano",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/armano2/freemarker-parser/issues"
  },
  "main": "index.js",
  "files": [
    "index.d.ts",
    "index.js",
    "index.js.map"
  ],
  "homepage": "https://github.com/armano2/freemarker-parser#readme",
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.26",
    "assert": "^1.4.1",
    "chalk": "^2.1.0",
    "codecov": "^2.3.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsesc": "^2.5.1",
    "mocha": "^3.5.0",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.1.0",
    "rollup": "^0.49.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-watch": "^4.3.1",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "wait-on": "^2.0.2",
    "warun": "^1.0.0"
  },
  "dependencies": {
    "debug": "^3.0.1",
    "line-column": "^1.0.2"
  }
}
