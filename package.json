{
  "name": "freemarker-parser",
  "version": "1.0.1",
  "description": "Freemarker Parser is a javascript implementation of the Freemarker",
  "scripts": {
    "build": "npm run lint & tsc",
    "pretest": "npm run build",
    "test": "nyc mocha --require ts-node/register \"test/*.ts\" --reporter=dot --timeout 5000",
    "posttest": "nyc report --reporter=lcov && codecov -f coverage/*.info",
    "lint": "tslint './src/**/*.ts'",
    "update-ast": "node ./scripts/update-ast.js",
    "watch": "tsc --watch",
    "coverage": "nyc npm run mocha && nyc report --reporter lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/armano2/freemarker-parser.git"
  },
  "keywords": [
    "freemarker",
    "parser",
    "js",
    "ast",
    "ftl"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "Armano (https://github.com/armano2)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/armano2/freemarker-parser/issues"
  },
  "main": "index.js",
  "files": [
    "index.d.ts",
    "index.js",
    "index.js.map"
  ],
  "homepage": "https://github.com/armano2/freemarker-parser#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "assert": "^1.4.1",
    "chalk": "^2.4.1",
    "codecov": "^3.1.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rollup": "^0.66.6",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-typescript2": "^0.17.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "debug": "^4.1.0",
    "lines-and-columns": "^1.1.6"
  }
}
